QToolBar#main_ui_toolbar {
    background-color: #505050;
    color: #f0f0f0;
    border: none;
}

QToolBar QToolButton {
    background-color: #505050;
    color: #f0f0f0;
    border: none;
}

QToolBar QToolButton:hover {
    background-color: #606060;
}

QToolBar QToolButton:pressed {
    background-color: #404040;
