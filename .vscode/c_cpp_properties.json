{
  "json.schemas": [
    {
      "fileMatch": [
        "/.vscode/c_cpp_properties.json"
      ],
      "url": "https://raw.githubusercontent.com/Microsoft/vscode-cpptools/master/Extension/schema/c_cpp_properties.schema.json"
    }
  ],
  "configurations": [
    {
      "name": "Linux",
      "includePath": [
        "${workspaceFolder}/**"
      ],
      "defines": [],
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "c17",
      "cppStandard": "c++17",
      "intelliSenseMode": "gcc-x64"
    }
  ],
  "version": 4
}